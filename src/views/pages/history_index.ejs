<div class="row">
    <div class="col-xl-12">
        <div class="card easion-card">
            <div class="card-header">
                <div class="easion-card-icon">
                    <i class="fas fa-user"></i>
                </div>
                <div class="easion-card-title">Lịch sử chỉnh sửa</div>
            </div>
            <div class="p-3">
                <table id="example" class="table table-border" style="width:100%">
                    <thead class="table-primary">
                        <tr>
                            <th>id người<br/>cập nhật</th>
                            <th>Ngày cập nhật</th>
                            <th>Nội dung</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        <% pageData[0].forEach((record,index)=>{ %>
                            <tr>
                                <td>
                                    <%= record.id_user%>
                                </td>
                                <td>
                                    <%= record.createdAt%>
                                </td>
                                <td>
                                    <%= record.history_content%>
                                </td>
                            </tr>
                            <%});%>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>